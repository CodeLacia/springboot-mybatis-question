<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hao.ums.mapper.AccountMapper">

    <select id="getAll" resultType="com.hao.ums.bean.Account">
        SELECT a.id as value,a.realname as label FROM dream.ums_account a where a.company_id = '1292702716514529281';
    </select>

    <select id="getOne" resultType="com.hao.ums.bean.Account">
        SELECT * FROM dream.ums_account where id = '1295911395615272961';
    </select>

</mapper>